#-*-Mode: Makefile-*-

SRCS =  AliFastJetFinder.cxx AliFastJetHeaderV1.cxx \
        AliSISConeJetFinder.cxx AliSISConeJetHeader.cxx
ifneq ($(FASTJET_ROOT),)
        EINCLUDE += $(FASTJET)_ROOT/include		
else	
	FASTJET_ROOT=$(ALICE_ROOT)/JETAN
        EINCLUDE += $(FASTJET_ROOT)/fastjet		
endif


HDRS:= $(SRCS:.cxx=.h) 
DHDR= FASTJETANLinkDef.h


ifeq (win32gcc,$(ALICE_TARGET))
PACKSOFLAGS:= $(SOFLAGS) -L$(ALICE_ROOT)/lib/tgt_$(ALICE_TARGET) \
                         -lSTEERBase -lESD -lAOD -lANALYSIS -lANALYSISalice -lJETAN \
                         -L$(shell root-config --libdir) -lEG -lGeom
endif

